        "containerId": "tpi-backend-realm-id"
      },
      {
        "id": "default-roles-id",
        "name": "default-roles-tpi-backend",
        "description": "${role_default-roles}",
        "composite": true,
        "composites": {
          "realm": [
            "offline_access",
            "uma_authorization"
          ]
        },
        "clientRole": false,
        "containerId": "tpi-backend-realm-id"
      }
    ]
  },
  "groups": [],
  "defaultRole": {
    "id": "default-roles-id",
    "name": "default-roles-tpi-backend",
